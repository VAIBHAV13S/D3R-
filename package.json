{
  "name": "blockchain-d3r",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd frontend && npm start",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "lint": "cd frontend && npm run lint",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "node scripts/clean.js",
    "wsl-clean": "bash -c './clean.sh'",
    "wsl-setup": "bash -c './setup.sh'",
    "test": "cd frontend && npm test",
    "test:backend": "cd backend && npm test"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "ethers": "^5.7.2",
    "next": "^13.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.5",
    "web3": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.8",
    "typescript": "^5.1.6"
  },
  "description": "Decentralized Disaster Donation & Relief Platform (D3R)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/blockchain-d3r.git"
  },
  "keywords": [
    "blockchain",
    "disaster-relief",
    "ethereum",
    "donations",
    "chainlink",
    "ipfs"
  ],
  "author": "D3R Team",
  "license": "MIT"
}
